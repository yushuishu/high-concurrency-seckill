<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.shuishu.demo.seckill.mapper.PaymentMapper">
    <resultMap id="paymentMapperResultMap" type="com.shuishu.demo.seckill.entity.Payment">
        <id column="payment_id" property="paymentId"/>
        <result column="seckill_success_id" property="seckillSuccessId"/>
        <result column="goods_inventory_id" property="goodsInventoryId"/>
        <result column="user_id" property="userId"/>
        <result column="payment_state" property="paymentState"/>
        <result column="payment_money" property="paymentMoney"/>
        <result column="payment_create_time" property="paymentCreateTime"/>
    </resultMap>

</mapper>